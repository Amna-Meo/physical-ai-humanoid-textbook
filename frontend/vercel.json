{
  "version": 2,
  "name": "physical-ai-textbook",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "https://physical-ai-textbook-backend.vercel.app/api/$1" },
    { "src": "/^(.*)\\.((?!js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|txt|json|xml|pdf|csv|map)$)", "dest": "/index.html" }
  ],
  "env": {
    "DATABASE_URL": "$DATABASE_URL",
    "GEMINI_API_KEY": "$GEMINI_API_KEY",
    "QDRANT_URL": "$QDRANT_URL",
    "QDRANT_API_KEY": "$QDRANT_API_KEY"
  }
}